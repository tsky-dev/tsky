    "prepublish": "pnpm run clean && pnpm run build"
  },
  "devDependencies": {
    "@tsky/lex-cli": "workspace:*",
    "@types/node": "^22.10.1",
    "fast-glob": "^3.3.2",
    "tar": "^7.4.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
